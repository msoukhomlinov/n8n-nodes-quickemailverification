{
  "name": "n8n-nodes-quickemailverification",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "description": "n8n node for QuickEmailVerification API",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "quickemailverification",
    "email",
    "validation"
  ],
  "license": "MIT",
  "homepage": "https://quickemailverification.com",
  "author": {
    "name": "Max Soukhomlinov",
    "email": "maxs@intellectit.com.au"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msoukhomlinov/n8n-nodes-quickemailverification.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc -w",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/QuickEmailVerificationApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/QuickEmailVerification/QuickEmailVerification.node.js"
    ]
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.16",
    "@types/request-promise-native": "~1.0.18",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.70.0",
    "prettier": "^2.8.8",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "keyv": "^4.5.3",
    "keyv-file": "5.1.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
