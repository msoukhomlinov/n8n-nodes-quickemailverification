{
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "Node",
		"target": "ES2022",
		"lib": ["ES2022"],
		"outDir": "./dist/",
		"rootDir": "./",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": [
		"credentials/**/*",
		"nodes/**/*",
		"nodes/**/*.json"
	],
}
